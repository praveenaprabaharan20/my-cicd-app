{
  "family": "my-flask-app",
  "containerDefinitions": [
    {
      "name": "flask-container",
      "image": "<ECR_REPO_URI>:latest",
      "essential": true,
      "portMappings": [
        { "containerPort": 8080, "hostPort": 8080 }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
